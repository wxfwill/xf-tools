{
  "name": "xf-tools",
  "version": "1.0.14",
  "author": "will",
  "main": "dist/xf-tools.umd.min.js",
  "module": "dist/xf-tools.esm.min.js",
  "license": "MIT",
  "keywords": [
    "xf-tools",
    "tools",
    "xf"
  ],
  "repository": "https://github.com/wxfwill/xf-tools.git",
  "scripts": {
    "rollup:dev": "rollup -c rollup.config.dev.js -w",
    "rollup:build": "rollup --config rollup.config.prod.js",
    "dev": "cross-env NODE_ENV=dev npm run rollup:dev",
    "build:test": "npm run clean && cross-env NODE_ENV=test npm run rollup:build",
    "build:prod": "npm run clean && cross-env NODE_ENV=prod npm run rollup:build",
    "build:copy": "npm run clean && cross-env NODE_ENV=copy npm run rollup:build && npm run copy",
    "build:publish": "npm run clean && cross-env NODE_ENV=publish npm run rollup:build && npm run copy && node scripts/publish.js",
    "pub": "npm run copy && node scripts/publish.js",
    "copy": "node scripts/copy.js",
    "clean": "rimraf dist"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "babel-loader": "^8.2.2",
    "commander": "^8.2.0",
    "copy": "^0.3.2",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.3.2",
    "node-cp": "^0.1.1",
    "postcss": "^8.3.8",
    "print-js": "^1.6.0",
    "rollup": "^2.57.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-server": "^0.7.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "shelljs": "^0.8.4",
    "terser-webpack-plugin": "^5.2.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0"
  }
}
